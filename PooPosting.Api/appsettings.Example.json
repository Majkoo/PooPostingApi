{
  "ConnectionStrings": {
    "DefaultConnection": "connectionStringSecret" 
  },
  "LogLevel": {
    "Default": "Warning",
    "Microsoft": "Trace",
    "Microsoft.Hosting.Lifetime": "Trace"
  },
  "Debug": {
    "LogLevel": {
      "Default": "Debug"
    }
  },
  "HashIdsSettings": {
    "MinHashLength": 10,
